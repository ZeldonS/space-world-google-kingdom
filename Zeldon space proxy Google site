  <input type="text" id="urlInput" placeholder="Enter GitHub URL" />
  <button id="goButton">Go</button>
</div>
<iframe id="browserFrame" sandbox="allow-scripts allow-same-origin" src=""></iframe>

<script>
  const urlInput = document.getElementById('urlInput');
  const goButton = document.getElementById('goButton');
  const iframe = document.getElementById('browserFrame');

  function loadURL() {
    let url = urlInput.value.trim();
    if (!url) {
      alert('Please enter a URL');
      return;
    }
    // Ensure URL starts with https://
    if (!url.startsWith('http')) {
      url = 'https://' + url;
    }
    // Optional: restrict to GitHub domains
    const allowedDomain = 'github.com';
    try {
      const parsedUrl = new URL(url);
      if (!parsedUrl.hostname.includes(allowedDomain)) {
        alert('Please enter a GitHub URL');
        return;
      }
    } catch (e) {
      alert('Invalid URL');
      return;
    }
    iframe.src = url;
  }

  goButton.addEventListener('click', loadURL);
  urlInput.addEventListener('keydown', (e) => {
    if (e.key === 'Enter') {
      loadURL();
    }
  });
</script>
</body>
